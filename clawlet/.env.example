# Clawlet environment variables
# Copy this file to .env and fill in your values.

# OpenRouter API key (required)
OPENROUTER_API_KEY=your_sk_or_sk-or-...

# Optional: override config file location
# CLAWLET_CONFIG=/path/to/config.yaml

# Logging
LOG_LEVEL=INFO
LOG_JSON=0
# LOG_FILE=/var/log/clawlet/clawlet.log  (or use agent --log-file)

# Timezone
TZ=UTC
